{
    "name": "fitness-logger",
    "version": "1.0.0",
    "main": "expo-router/entry",
    "scripts": {
        "start": "expo start",
        "android": "expo start --android",
        "ios": "expo start --ios",
        "web": "expo start --web",
        "server": "nodemon my_server_file.js",
        "test": "jest --watchAll --runInBand --detectOpenHandles --verbose",
        "lint": "npx eslint ."
    },
    "jest": {
        "preset": "jest-expo",
        "transformIgnorePatterns": [
            "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@sentry/react-native|native-base|react-native-svg|@realm)"
        ],
        "transform": {
            "^.+\\.jsx?$": "babel-jest"
        },
        "moduleNameMapper": {
            "react-native-compressor": "<rootDir>/__mocks__/react-native-compressor.js"
        }
    },
    "dependencies": {
        "@react-native-async-storage/async-storage": "1.23.1",
        "@react-native-community/datetimepicker": "^8.0.1",
        "@react-navigation/drawer": "^6.7.2",
        "@react-navigation/native": "^6.1.18",
        "@realm/react": "^0.11.0",
        "@testing-library/react-native": "^12.7.2",
        "babel-preset-expo": "^11.0.14",
        "expo": "~51.0.28",
        "expo-dev-client": "~4.0.26",
        "expo-file-system": "~17.0.1",
        "expo-linking": "~6.3.1",
        "expo-router": "~3.5.23",
        "jest": "^29.4.0",
        "jest-expo": "~51.0.4",
        "jest-mock": "^29.7.0",
        "nativewind": "^2.0.11",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "react-hook-form": "^7.53.0",
        "react-native": "0.74.5",
        "react-native-chart-kit": "^6.12.0",
        "react-native-collapsible": "^1.6.2",
        "react-native-compressor": "^1.9.0",
        "react-native-document-picker": "^9.3.1",
        "react-native-element-dropdown": "^2.12.1",
        "react-native-fs": "^2.20.0",
        "react-native-gesture-handler": "~2.16.1",
        "react-native-reanimated": "~3.10.1",
        "react-native-reanimated-table": "^0.0.2",
        "react-native-safe-area-context": "4.10.5",
        "react-native-screens": "3.31.1",
        "react-native-share": "^12.0.9",
        "react-native-svg": "15.2.0",
        "react-native-table-component": "^1.2.2",
        "react-native-vector-icons": "^10.2.0",
        "react-native-video": "^6.9.1",
        "react-native-web": "~0.19.10",
        "react-test-renderer": "18.2.0",
        "realm": "^20.0.0"
    },
    "devDependencies": {
        "@babel/core": "^7.20.0",
        "@stylistic/eslint-plugin": "^2.7.2",
        "@stylistic/eslint-plugin-js": "^2.7.2",
        "@stylistic/eslint-plugin-jsx": "^2.7.2",
        "eslint": "^9.10.0",
        "eslint-plugin-json-format": "^2.0.1",
        "husky": "^9.1.5",
        "nodemon": "^3.1.4",
        "tailwindcss": "3.3.2"
    },
    "private": true
